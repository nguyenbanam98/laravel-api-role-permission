{
    "name": "wesimplycode/laravel-api-role-permission",
    "description": "Simple role-permission package for api's build with the Laravel Framework.",
    "keywords": [
        "wesimplycode",
        "laravel",
        "api",
        "permission",
        "permissions",
        "role",
        "roles"
    ],
    "type": "library",
    "require": {
        "php": ">=7.2",
        "illuminate/auth": "^5.8|^6.0|^7.0",
        "illuminate/container": "^5.8|^6.0|^7.0",
        "illuminate/contracts": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "WeSimplyCode\\LaravelApiRolePermission\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeSimplyCode\\LaravelApiRolePermission\\LaravelApiRolePermissionServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sunil Kisoensingh",
            "email": "sunil.kisoensingh@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
